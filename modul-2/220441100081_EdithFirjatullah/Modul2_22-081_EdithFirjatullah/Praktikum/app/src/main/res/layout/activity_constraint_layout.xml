<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fon"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConstraintLayout">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/logoutm"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="23dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/logoutm"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
        android:fontFamily="@font/poppins_bold"
        android:id="@+id/namauniv"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="23dp"
        android:layout_width="wrap_content"
        android:text="Universitas Trunojoyo"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

        <TextView
        android:fontFamily="@font/poppins"
        android:id="@+id/beranda"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:text="Beranda"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/namauniv"
        app:layout_constraintTop_toBottomOf="@+id/namauniv" />

        <TextView
            android:id="@+id/aktivitas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="36dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Aktivitas Kegiatan"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="@+id/namauniv"
            app:layout_constraintTop_toBottomOf="@+id/beranda" />

        <ImageView
            android:id="@+id/gambar1"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginTop="12dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/namauniv"
            app:layout_constraintTop_toBottomOf="@+id/aktivitas" />

        <ImageView
            android:id="@+id/gambar2"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginTop="27dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/namauniv"
            app:layout_constraintTop_toBottomOf="@+id/gambar1"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/gambar3"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginStart="27dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="18dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image5"
            app:layout_constraintBottom_toBottomOf="@+id/gambar1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/gambar1"
            app:layout_constraintTop_toBottomOf="@+id/aktivitas" />

        <ImageView
            android:id="@+id/gambar4"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginStart="27dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image5"
            app:layout_constraintBottom_toBottomOf="@+id/gambar2"
            app:layout_constraintStart_toEndOf="@+id/gambar2"
            app:layout_constraintTop_toTopOf="@+id/gambar2"
            tools:ignore="MissingConstraints" />

        <TextView
        android:fontFamily="@font/poppins_semibold"
        android:id="@+id/artikel"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_width="wrap_content"
        android:text="Artikel"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/namauniv"
        app:layout_constraintTop_toBottomOf="@+id/gambar2" />

        <ImageView
            android:id="@+id/gbrartikel1"
            android:layout_width="116dp"
            android:layout_height="116dp"
            android:layout_marginTop="15dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image4"
            app:layout_constraintStart_toStartOf="@+id/namauniv"
            app:layout_constraintTop_toBottomOf="@+id/artikel" />

        <TextView
        android:fontFamily="@font/poppins_medium"
        android:id="@+id/judulartikel"
        android:layout_height="wrap_content"
        android:layout_marginEnd="18dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="26dp"
        android:layout_width="0dp"
        android:text="Terlalu Jago, Trunojoyo Juara Lagi"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/gbrartikel1"
        app:layout_constraintTop_toBottomOf="@+id/artikel" />

        <TextView
        android:fontFamily="@font/poppins_light"
        android:id="@+id/isiartikel"
        android:layout_height="wrap_content"
        android:layout_marginEnd="18dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="15dp"
        android:layout_width="0dp"
        android:text="Universitas Trunojoyo berhasil meraih juara 1 dalam ajang perlombaan debat nasional yang diselenggarakan oleh..."
        android:textColor="@color/black"
            android:justificationMode="inter_word"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/gbrartikel1"
        app:layout_constraintTop_toBottomOf="@+id/judulartikel" />

        <ImageView
            android:id="@+id/gbrartikel2"
            android:layout_width="116dp"
            android:layout_height="116dp"
            android:layout_marginTop="26dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image4"
            app:layout_constraintStart_toStartOf="@+id/gbrartikel1"
            app:layout_constraintTop_toBottomOf="@+id/gbrartikel1" />

        <TextView
            android:id="@+id/isiartikel2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="18dp"
            android:fontFamily="@font/poppins_light"
            android:text="Universitas Trunojoyo berhasil meraih juara 1 dalam ajang perlombaan debat nasional yang diselenggarakan oleh..."
            android:textColor="@color/black"
            android:justificationMode="inter_word"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/judulartikel2"
            app:layout_constraintTop_toBottomOf="@+id/judulartikel2" />

        <ImageView
            android:id="@+id/gbrartikel3"
            android:layout_width="116dp"
            android:layout_height="116dp"
            android:layout_marginTop="26dp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/image4"
            app:layout_constraintStart_toStartOf="@+id/gbrartikel2"
            app:layout_constraintTop_toBottomOf="@+id/gbrartikel2" />

        <TextView
            android:id="@+id/isiartikel3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="18dp"
            android:fontFamily="@font/poppins_light"
            android:text="Universitas Trunojoyo berhasil meraih juara 1 dalam ajang perlombaan debat nasional yang diselenggarakan oleh..."
            android:textColor="@color/black"
            android:justificationMode="inter_word"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/judulartikel3"
            app:layout_constraintTop_toBottomOf="@+id/judulartikel3"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/judulartikel2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:layout_marginTop="11dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Terlalu Jago, Trunojoyo Juara Lagi"
            android:textColor="@color/black"
            android:textSize="13sp"
            app:layout_constraintStart_toEndOf="@+id/gbrartikel2"
            app:layout_constraintTop_toTopOf="@+id/gbrartikel2"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/judulartikel3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:layout_marginTop="11dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Terlalu Jago, Trunojoyo Juara Lagi"
            android:textColor="@color/black"
            android:textSize="13sp"
            app:layout_constraintStart_toEndOf="@+id/gbrartikel3"
            app:layout_constraintTop_toTopOf="@+id/gbrartikel3"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
